msgid "ENCRYPTION LEVEL: TWO"
msgstr ""

#. STRINGS.CODEX.ARTHISTORYREQUEST.TITLE
msgctxt "STRINGS.CODEX.ARTHISTORYREQUEST.TITLE"
msgid "Re: Implant Database Request"
msgstr ""

