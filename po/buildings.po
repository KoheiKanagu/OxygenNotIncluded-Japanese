msgid "Sends an <link=\"LOGIC\">Active</link> signal when full"
msgstr "設定値を越えると<link=\"LOGIC\">アクティブ</link>になります"

#. STRINGS.BUILDINGS.PREFABS.REFRIGERATOR.LOGIC_PORT_INACTIVE
msgctxt "STRINGS.BUILDINGS.PREFABS.REFRIGERATOR.LOGIC_PORT_INACTIVE"
msgid "Sends a <link=\"LOGIC\">Standby</link> signal when not full"
msgstr ""

